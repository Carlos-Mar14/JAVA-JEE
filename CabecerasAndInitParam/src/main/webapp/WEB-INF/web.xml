<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <servlet>
    <description></description>
    <display-name>Cabeceras</display-name>
    <servlet-name>Cabeceras</servlet-name>
    <servlet-class>servlets.Cabeceras</servlet-class>
    <init-param>
      <param-name>ciudad</param-name>
      <param-value>Barcelona</param-value>
    </init-param>
    <init-param>
      <param-name>moneda</param-name>
      <param-value>euro</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cabeceras</servlet-name>
    <url-pattern>/cabeceras</url-pattern>
  </servlet-mapping>
  <display-name>CabecerasAndInitParam</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>